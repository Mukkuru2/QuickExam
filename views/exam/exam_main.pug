doctype html
html(lang='en')
    include ../common/head
    +head("Emails")
    body
        header
            include ../common/navbar
            +navbar(user)
            .container
                .row.align-items-center
                    .col-md-9.order-md-1.text-center.text-md-left.pr-md-5
                    h1 Emails
                    table.table
                        thead
                            tr
                                th(scope="col") Email
                                th(scope="col") Edit Email
                                th(scope="col")
                            tr
                        - var n = 0;
                        while n < 10
                            tbody
                                tr(id="q" + n++)
                                    td
                                        span Yes
                                    td
                                        span Edit
                                    td
                                        span Delete
n